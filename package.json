{
  "name": "spaceshow",
  "version": "1.1.0",
  "description": "Wrapper for easily testing meteor apps with puppeteer",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "lint": "eslint src",
    "build": "babel src -d dist",
    "clean": "rm -rf dist",
    "pretest": "npm run lint && npm run build",
    "test": "ava"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxnowack/spaceshow.git"
  },
  "keywords": [
    "meteor",
    "testing",
    "puppeteer",
    "chrome"
  ],
  "author": "Max Nowack",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxnowack/spaceshow/issues"
  },
  "homepage": "https://github.com/maxnowack/spaceshow#readme",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "psext": "0.0.4",
    "puppeteer": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}
